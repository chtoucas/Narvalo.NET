{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Narvalo.Cerbere/Narvalo.Cerbere.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "toc.yml", "**/*.yml" ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [
          "*.{md,yml}",
          "changelogs/**/*.{md,yml}",
          "engineering/**/*.{md,yml}",
          "userdocs/**/*.{md,yml}",
          "code-analysis.txt",
          "code-contracts.txt",
          "code-coverage.html",
        ]
      }
    ],
    "resource": [],
    "overwrite": [],
    "dest": "www",
    "globalMetadata": {
      "_appTitle": "Narvalo.NET website",
      "_appFooter": "Copyright (c) Narvalo.Org",
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "xref": ["../packages/msdn.4.5.2.0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"],
    "noLangKeyword": false
  }
}