<?xml version="1.0" encoding="utf-8" ?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NuId>Narvalo.Build</NuId>
    <NuDescription>
      <![CDATA[
Provides custom MSBuild tasks: download a file from internet, merge files, helpers to generate and verify assembly versions, tasks for Google Closure Compiler and YUICompressor.
      ]]>
    </NuDescription>
    <NuTags>MSBuild, Closure, YUI, Versioning</NuTags>

    <NuGetCommandAdditionalArgs>-Tool</NuGetCommandAdditionalArgs>

    <NuFiles>
      <![CDATA[
<file src="$(MSBuildThisFileDirectory)build\Narvalo.Build.tasks" target="build\" />
<file src="Narvalo.Build.dll" target="tools\" />
      ]]>
    </NuFiles>
  </PropertyGroup>
</Project>