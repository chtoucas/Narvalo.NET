<#

// Country codes sorted in alphabetic english order.
var codeSet = new HashSet<string> { };

var source = VSHost.ResolvePath(@"..\..\..\data\ISO3166\alpha2.txt");
var codes = File.ReadAllLines(source);

foreach (var code in codes)
{
    codeSet.Add(code);
}

WriteHeader();

