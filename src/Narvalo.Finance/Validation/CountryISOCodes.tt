        {
            get
            {
                if (s_TwoLetterCodeSet == null)
                {
                    var codeSet = new HashSet<string>() {
        #region Long list of alpha-2 codes sorted in alphabetic order.

<# foreach (var code in codeSet.OrderBy(_ => _)) { #>
                        "<#= code #>",
<# } #>

        #endregion
                    };

                    s_TwoLetterCodeSet = codeSet;
                }

                return s_TwoLetterCodeSet;
            }
        }
    }
}