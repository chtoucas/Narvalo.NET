<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- 
    This file is injected at the beginnig of Narvalo.Common.props by Narvalo.proj.
    
    Global properties:
    - NarvaloAdditionalConstants
  -->

  <!-- Compilation symbols added by Narvalo.proj. -->
  <PropertyGroup>
    <DefineConstants Condition=" '$(NarvaloAdditionalConstants)' != '' ">$(NarvaloAdditionalConstants);$(DefineConstants)</DefineConstants>
  </PropertyGroup>

  <ItemGroup Condition=" '$(NarvaloAssemblyInfoFile)' != '' ">
    <Compile Include="$(NarvaloAssemblyInfoFile)">
      <Link>Properties\AssemblyInfo.Version.cs</Link>
    </Compile>
  </ItemGroup>
</Project>