<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <VisualStudioVersion Condition=" '$(VisualStudioVersion)' == '' ">12.0</VisualStudioVersion>

    <NarvaloVerbose Condition=" '$(NarvaloVerbose)' != 'false' ">true</NarvaloVerbose>

    <NarvaloMessageImportance Condition=" '$(NarvaloVerbose)' == 'true' ">High</NarvaloMessageImportance>
    <NarvaloMessageImportance Condition=" '$(NarvaloVerbose)' != 'true' ">Normal</NarvaloMessageImportance>

    <NarvaloRepositoryRoot>$(MSBuildThisFileDirectory)..\</NarvaloRepositoryRoot>
    <NarvaloSettingsDir>$(NarvaloRepositoryRoot)etc\</NarvaloSettingsDir>
    <NarvaloToolsDir>$(NarvaloRepositoryRoot)tools\</NarvaloToolsDir>
  </PropertyGroup>
</Project>